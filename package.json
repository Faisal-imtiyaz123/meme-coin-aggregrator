{
  "name": "meme-coin-aggregator",
  "version": "1.0.0",
  "description": "Real-time meme coin data aggregation service",
  "main": "dist/app.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest tests/integration",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "bull": "^4.11.0",
    "cli-table3": "^0.6.5",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.0",
    "express-rate-limit": "^6.8.1",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.0",
    "joi": "^17.9.0",
    "rate-limiter-flexible": "^2.4.0",
    "redis": "^5.9.0",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.8.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.25",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.25",
    "@types/supertest": "^6.0.3",
    "axios-mock-adapter": "^2.1.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "crypto",
    "defi",
    "websocket",
    "redis",
    "aggregation"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./src/tests/setup.ts"
    ],
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/?(*.)+(spec|test).ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/tests/**"
    ]
  },
  "author": "Your Name",
  "license": "MIT"
}
